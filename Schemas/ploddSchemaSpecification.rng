  <sch:ns xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" prefix="xi" uri="http://www.w3.org/2001/XInclude"/>
  <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="ploddSchemaSpecification-no_XInclude-constraint-rule-51">
    <sch:rule xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" context="/">
      <sch:report test="//xi:*" role="fatal">There should be no XInclude elements in a derived PLODD</sch:report>
    </sch:rule>
