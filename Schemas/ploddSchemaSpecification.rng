  <define name="plodd_defaultVal">
    <element name="defaultVal">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(default value) specifies the default declared value for an attribute. [22.5.3. Attribute List Specification]</a:documentation>
      <text/>
      <ref name="plodd_att.global.attribute.xmlid"/>
      <ref name="plodd_att.global.attribute.n"/>
      <ref name="plodd_att.global.attribute.xmllang"/>
      <ref name="plodd_att.global.attribute.xmlbase"/>
      <ref name="plodd_att.global.attribute.xmlspace"/>
