  <define name="plodd_moduleRef">
    <element name="moduleRef">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(module reference) references a module which is to be incorporated into a schema. [22.2. Modules and Schemas]</a:documentation>
      <ref name="plodd_content"/>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron" xmlns:rng="http://relaxng.org/ns/structure/1.0" id="ploddSchemaSpecification-moduleRef-only_rng_in_moduleRef_content-constraint-rule-19">
        <sch:rule xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:xi="http://www.w3.org/2001/XInclude" context="tei:moduleRef/tei:content">
          <sch:report test="*[ not( self::rng:* or self::a:* ) ]">
