  <define name="plodd_valItem">
    <element name="valItem">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">documents a single value in a predefined list of values. [22.5.3. Attribute List Specification]</a:documentation>
      <ref name="plodd_gloss_amper_desc"/>
      <ref name="plodd_att.global.attribute.xmlid"/>
      <ref name="plodd_att.global.attribute.n"/>
      <ref name="plodd_att.global.attribute.xmllang"/>
      <ref name="plodd_att.global.attribute.xmlbase"/>
      <ref name="plodd_att.global.attribute.xmlspace"/>
