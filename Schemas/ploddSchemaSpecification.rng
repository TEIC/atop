          <ref name="plodd_model.ptrLike"/>
          <ref name="plodd_model.attributable"/>
          <ref name="plodd_q"/>
        </choice>
      </oneOrMore>
      <ref name="plodd_att.global.attributes"/>
      <ref name="plodd_att.typed.attributes"/>
      <empty/>
    </element>
  </define>
  <define name="plodd_mentioned">
