#
# Base environment variables
#

# Latest Ant
ANT_VERSION=1.10.11

# Latest BaseX
BASEX_VERSION=9.6.1

# Do not perform Maven package by default
DO_MAVEN_PACKAGE=

# Log4j is for XML Calabash 1.3.2 or later
LOG4J_VERSION=2.14.1

# XML Calabash is not always available depending on Saxon version
XMLCALABASH_VERSION=

# Latest XML Resolver
XML_RESOLVER_VERSION=1.2
