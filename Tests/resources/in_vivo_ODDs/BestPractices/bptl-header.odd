        <div type="specification">
          <head>Specification</head>
          <specGrp xml:id="libHeadSpec" xml:lang="en">
            <classSpec module="tei"
                       ident="model.encodingDescPart"
                       type="model"
                       mode="delete"/>
            <classSpec module="tei"
                       ident="model.teiHeaderPart"
                       type="model"
                       mode="delete"/>
            <!-- First, some constraints that we have no where else to put, as we make no -->
            <!-- other modifications to the relevant elements. -->
            <constraintSpec scheme="schematron" ident="no-extent-in-fileDesc">
              <constraint>
                <s:rule context="tei:fileDesc">
                  <s:report test="tei:extent">The &lt;extent&gt; element
                  should not be used as a direct child of
                  &lt;fileDesc&gt;, but rather only as a descendant of a
                  bibliographic citation (e.g., of the source).</s:report>
                </s:rule>
              </constraint>
            </constraintSpec>
            <constraintSpec scheme="schematron" ident="when-or-before-after">
              <constraint>
                <s:rule context="tei:imprint/tei:date">
                  <s:assert test="@when or ( @notBefore and @notAfter )">
                    When inside an &lt;imprint&gt;, a &lt;date&gt;
                    element should have a machine-readable version
                    of the date specified either on the @when
                    attribute, or, in the case of not knowing the
                    precise date, on the @notBefore and @notAfter
                  attributes.</s:assert>
                </s:rule>
              </constraint>
            </constraintSpec>
            <constraintSpec scheme="schematron" ident="onlyAllowedAttrs">
              <constraint>
                <s:ns uri="http://www.tei-c.org/ns/1.0" prefix="tei"/>
                <s:rule context="tei:*/@*">
                  <s:assert test="name(.) = ('anchored', 'baseForm',
                  'baseTypes', 'calendar', 'cause', 'cert', 'class',
                  'cols', 'copyOf', 'corresp', 'degree', 'dur',
                  'dur-iso', 'ed', 'end', 'eol', 'evaluate',
                  'exclude', 'facs', 'from', 'from-iso', 'full', 'gi',
                  'given', 'group', 'hand', 'height', 'ident', 'key',
                  'level', 'locus', 'marks', 'met', 'min', 'n',
                  'name', 'next', 'notAfter', 'notBefore', 'org',
                  'part', 'period', 'place', 'precision', 'prev',
                  'quantity', 'reason', 'ref', 'rend', 'render',
                  'rendition', 'resp', 'rhyme', 'role', 'rows', 'sex',
                  'scale', 'scheme', 'scope', 'select', 'sort',
                  'subtype', 'synch', 'target', 'targetEnd',
                  'targets', 'targFunc', 'to', 'to-iso', 'type',
                  'unit', 'uri', 'url', 'usage', 'version', 'when',
                  'who', 'width', 'lrx', 'lry', 'ulx', 'uly',
                  'xml:base', 'xml:id', 'xml:lang', 'xml:space')">The
                  use of the <s:name/>= attribute (found here on the
                  &lt;<s:value-of select="local-name(..)"/>&gt;
                  element) is not recommended by the Best Practices
                  for TEI in Libraries</s:assert>
                </s:rule>
              </constraint>
            </constraintSpec>
            <elementSpec module="core" ident="relatedItem" mode="change">
              <classes mode="replace">
                <memberOf key="att.global"/>
                <memberOf key="model.biblPart"/>
                <memberOf key="att.typed"/>
              </classes>
              <content>
                <elementRef key="biblStruct"/>
              </content>
              <constraintSpec ident="targetorcontent1" mode="delete" scheme="schematron"/>
              <attList>
                <attDef ident="target" mode="delete"/>
              </attList>
            </elementSpec>
            <elementSpec module="core" ident="respStmt" mode="change">
              <content>
                <sequence preserveOrder="false">
                  <elementRef minOccurs="1" maxOccurs="1" key="resp"/>
                  <classRef minOccurs="1" maxOccurs="unbounded" key="model.nameLike.agent"/>
                </sequence>
              </content>
            </elementSpec>
            <moduleRef key="tagdocs" include="att val gi tag"/>
            <moduleRef key="header"
                       include="appInfo application availability change classCode classDecl correction distributor edition editionStmt editorialDecl encodingDesc extent fileDesc hyphenation idno keywords langUsage language licence listPrefixDef namespace normalization notesStmt prefixDef profileDesc projectDesc publicationStmt punctuation quotation rendition revisionDesc samplingDecl schemaRef seriesStmt sourceDesc tagUsage tagsDecl taxonomy teiHeader textClass titleStmt"/>
            <elementSpec module="header" ident="change" mode="change">
              <!-- constraintSpec scheme="schematron"
                   ident="revChronOrder"> </constraintSpec -->
              <attList>
                <attDef mode="change" ident="when" usage="req"/>
                <attDef mode="change" ident="who" usage="req"/>
              </attList>
            </elementSpec>
            <elementSpec module="header" ident="schemaRef" mode="change">
              <attList>
                <attDef ident="key" mode="delete"/>
                <attDef ident="url" mode="change">
                  <datatype minOccurs="1" maxOccurs="1">
                    <dataRef key="teidata.enumerated"/>
                  </datatype>
                  <!-- When you change the version # here, remember to change it
                       in the <editionStmt>s in each of the .odd
                       files, too! -->
                  <valList type="semi" mode="add">
                    <valItem ident="bptl:L1-v4.0.0">
                      <gloss>Best Practices for TEI in Libraries level 1, version 4.0.0</gloss>
                    </valItem>
                    <valItem ident="bptl:L2-v4.0.0">
                      <gloss>Best Practices for TEI in Libraries level 2, version 4.0.0</gloss>
                    </valItem>
                    <valItem ident="bptl:L3-v4.0.0">
                      <gloss>Best Practices for TEI in Libraries level 3, version 4.0.0</gloss>
                    </valItem>
                    <valItem ident="bptl:L4-v4.0.0">
                      <gloss>Best Practices for TEI in Libraries level 4, version 4.0.0</gloss>
                    </valItem>
                    <valItem ident="bptl:L5-v4.0.0">
                      <gloss>Best Practices for TEI in Libraries level 5, version 4.0.0</gloss>
                    </valItem>
                  </valList>
                </attDef>
              </attList>
            </elementSpec>
            <elementSpec ident="hyphenation-para" mode="add">
              <altIdent>p</altIdent>
              <content>
                <valList type="closed">
                  <valItem ident="All hyphens in source document encoded as U+2010."/>
                  <valItem ident="All hyphens in source document encoded as U+002D."/>
                  <valItem ident="Hyphens in source document
                                  encoded as U+2010. Line-ending hyphens
                                  differentiated with @break of immediately following
                                  &lt;lb&gt; element."/>
                  <valItem ident="Hyphens in source document
                                  encoded as U+002D. Line-ending hyphens
                                  differentiated with @break of immediately following
                                  &lt;lb&gt; element."/>
                  <valItem ident="Hyphens in source document encoded as
                                  U+2010 inside a &lt;pc&gt;. Line-ending hyphens
                                  differentiated with @force of the &lt;pc&gt; and
                                  @break of immediately following &lt;lb&gt;
                                  element."/>
                  <valItem ident="Hyphens in source document encoded as
                                  U+202D inside a &lt;pc&gt;. Line-ending hyphens
                                  differentiated with @force of the &lt;pc&gt; and
                                  @break of immediately following &lt;lb&gt;
                                  element."/>
                </valList>
              </content>
            </elementSpec>
            <elementSpec module="header" ident="hyphenation" mode="change">
              <content>
                <alternate>
                  <sequence>
                    <elementRef key="hyphenation-para" minOccurs="1" maxOccurs="1"/>
                    <elementRef key="p" minOccurs="0" maxOccurs="unbounded"/>
                  </sequence>
                  <sequence>
                    <elementRef key="p" minOccurs="1" maxOccurs="unbounded"/>
                  </sequence>
                </alternate>
              </content>
            </elementSpec>
            <elementSpec module="header" ident="editorialDecl" mode="change">
              <content>
                <sequence>
                  <elementRef key="correction"    minOccurs="0" maxOccurs="1"/>
                  <elementRef key="hyphenation"   minOccurs="0" maxOccurs="1"/>
                  <elementRef key="normalization" minOccurs="0" maxOccurs="1"/>
                  <elementRef key="punctuation"   minOccurs="0" maxOccurs="1"/>
                  <elementRef key="quotation"     minOccurs="0" maxOccurs="1"/>
                  <classRef key="model.pLike"     minOccurs="0" maxOccurs="7"/>
                </sequence>
              </content>
              <exemplum xml:lang="en">
                <egXML xmlns="http://www.tei-c.org/ns/Examples">
                  <editorialDecl>
                    <p>Metadata in the TEI header comes from an AACR2-conformant
                    record, translated to TEI via the <name type="software">Thutmose I</name> program.</p>
                    <p>Content originally generated by <name type="software">c-n-rite</name> OCR software, then the
                    needed TEI encoding put in place with <name type="software">cnr2tei.xslt</name>.</p>
                    <correction method="markup" status="low"/>
                    <normalization method="silent">
                      <p>Spacing between words and the following
                      punctuation mark has been removed. Spacing
                      between words has generally been regularized to
                      one space.</p>
                    </normalization>
                    <normalization method="markup">
                      <p>Distances expressed in imperial units in the
                      source have been normalized to the metric system
                      using the attributes of the <gi>measure</gi>
                      element.</p>
                    </normalization>
                    <hyphenation eol="all">
                      <p>All hyphens in source document encoded as U+2010.</p>
                    </hyphenation>
                  </editorialDecl>
                </egXML>
              </exemplum>
          </elementSpec>
          <elementSpec module="header" ident="encodingDesc" mode="change">
            <content>
              <sequence>
                <elementRef key="projectDesc"   minOccurs="0" maxOccurs="1"/>
                <elementRef key="schemaRef"     minOccurs="0" maxOccurs="unbounded"/>
                <elementRef key="editorialDecl" minOccurs="0" maxOccurs="1"/>
                <elementRef key="tagsDecl"      minOccurs="1" maxOccurs="1"/>
                <elementRef key="classDecl"     minOccurs="0" maxOccurs="1"/>
