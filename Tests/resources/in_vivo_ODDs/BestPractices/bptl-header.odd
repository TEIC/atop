          </elementSpec>
          <elementSpec ident="BPTL.required.prefixDef" mode="add">
            <altIdent>prefixDef</altIdent>
            <attList>
              <attDef ident="ident" usage="req">
                <valList type="closed">
                  <valItem ident="bptl">
                    <gloss>Best Practices for TEI in Libraries</gloss>
                  </valItem>
                </valList>
              </attDef>
              <attDef ident="matchPattern" usage="req">
                <valList type="closed">
                  <valItem ident="L([1-5])-v(\d+\.\d+\.\d+[aαβb]?)">
                    <desc>This regular expression matches BPTL version
                    numbers like <q>L1-v1.0.2a</q>.</desc>
                  </valItem>
                </valList>
              </attDef>
              <attDef ident="replacementPattern" usage="req">
                <valList type="semi">
                  <valItem ident="http://www.tei-c.org/SIG/Libraries/teiinlibraries/$2/"/>
                </valList>
              </attDef>
            </attList>
          </elementSpec>              
          <elementSpec module="header" ident="listPrefixDef" mode="change">
            <content>
              <elementRef key="BPTL.required.prefixDef"/>
              <alternate minOccurs="0" maxOccurs="unbounded">
                <elementRef key="prefixDef"/>
                <elementRef key="listPrefixDef"/>
              </alternate>
            </content>
          </elementSpec>
          <elementSpec module="header" ident="namespace" mode="change">
            <content>
              <elementRef key="tagUsage"/>
            </content>
            <attList>
              <attDef ident="name" mode="change">
                <datatype minOccurs="1" maxOccurs="1">
                  <dataRef key="lib.teins"/>
                </datatype>
              </attDef>
            </attList>
          </elementSpec>
          <dataSpec ident="lib.teins">
            <content>
              <valList type="closed">
                <valItem ident="http://www.tei-c.org/ns/1.0"/>
              </valList>
            </content>
          </dataSpec>
          <elementSpec module="header" ident="publicationStmt" mode="change">
            <constraintSpec scheme="schematron" ident="PubStmt-specialPreferred">
              <constraint>
                <s:report role="warning" test="child::tei:p">Use of
                specialized child elements of the publication
                statement (rather than paragraphs) is recommended
                whenever possible</s:report>
              </constraint>
            </constraintSpec>
            <constraintSpec scheme="schematron" ident="whenNotContent">
              <constraint>
                <s:rule context="tei:publicationStmt/tei:date">
                  <s:assert test="@when">Dates inside the
                  publication statement should have @when
                  (and should not have content)</s:assert>
                  <s:report test="string-length(
                                  normalize-space(.) ) &gt; 0">Dates
                  inside the publication statement should
                  not have content (and should have
                  @when)</s:report>
                </s:rule>
              </constraint>
            </constraintSpec>
            <remarks>
              <p>For normally published items, the specialized children
              elements (e.g. <gi>publisher</gi>) should be used.
              Paragraphs are permitted as an alternative for unusual
              cases like unpublished works.</p>
            </remarks>
          </elementSpec>
          <elementSpec module="header" ident="rendition" mode="change">
            <attList>
              <attDef ident="scheme" mode="change" usage="req">
                <valList type="closed" mode="replace">
                  <valItem ident="css">
                    <desc>Cascading Stylesheet Language</desc>
                  </valItem>
                </valList>
              </attDef>
            </attList>
          </elementSpec>
          <elementSpec module="header" ident="sourceDesc" mode="change">
            <content>
              <elementRef key="biblStruct"/>
            </content>
          </elementSpec>
          <elementSpec module="header" ident="tagsDecl" mode="change">
            <content>
              <sequence>
                <elementRef key="rendition" minOccurs="0" maxOccurs="unbounded"/>
                <elementRef key="namespace"/>
              </sequence>
            </content>
          </elementSpec>
          <elementSpec module="header" ident="tagUsage" mode="change">
            <content>
              <!-- note: having explicit <dataRef key="teidata.enumerated"/> causes invalid RELAX NG output -->
              <valList type="closed">
                <valItem ident="Unnumbered divs used."/>
                <valItem ident="Numbered divs used."/>
                <valItem ident="Logical divisions not encoded."/>
              </valList>
            </content>
            <attList>
              <attDef ident="gi" mode="replace" usage="req">
                <gloss>element name</gloss>
                <desc>the name (generic identifier) of the element indicated by the tag.</desc>
                <datatype>
                  <dataRef key="teidata.enumerated"/>
                </datatype>
                <constraintSpec scheme="schematron" ident="numbered-vs-unnumbered">
                  <constraint>
                    <s:rule context="tei:text">
                      <s:report test=".//tei:div and .//tei:div1">
                        A single TEI in Libraries document should not mix numbered and unnumbered divisions.
                      </s:report>
                    </s:rule>
                    <s:rule context="/tei:TEI[ tei:text//tei:div or tei:text//tei:div1 ]">
                      <s:assert test="/tei:TEI/tei:teiHeader/tei:encodingDesc/tei:tagsDecl/tei:namespace/tei:tagUsage[ @gi = ('div','div1') ]">
                        The use of divisions (i.e., whether numbered or
                        unnumbered divisions are used) should be
                        documented in a &lt;tagUsage&gt; element by "Unnumbered divs used." or "Numbered divs used." The &lt;tagUsage&gt; element must be the child of
                        a &lt;namespace name="http://www.tei-c.org/ns/1.0"&gt; element.</s:assert>
                    </s:rule>
                    <s:rule context="/tei:TEI[ tei:sourceDoc and not( tei:text )]">
                      <s:assert test="/tei:TEI/tei:teiHeader/tei:encodingDesc/tei:tagsDecl/tei:namespace/tei:tagUsage[ @gi eq 'sourceDoc' ] ">
                        The fact that logical divisions are not encoded (and thus neither numbered nor unnumbered divisions are used) should be
                        documented in a &lt;tagUsage&gt; element by "Logical divisions not encoded." The &lt;tagUsage&gt; element must be the child of
                        a &lt;namespace name="http://www.tei-c.org/ns/1.0"&gt; element.</s:assert>
                    </s:rule>
                    <s:rule context="tei:tagUsage">
                      <s:assert test="
                                ( @gi eq 'div' and normalize-space(.) eq 'Unnumbered divs used.' and not( /tei:TEI/tei:text//tei:div1 ) )
                                or
                                ( @gi eq 'div1' and normalize-space(.) eq 'Numbered divs used.' and not( /tei:TEI/tei:text//tei:div ) )
                                or
                                ( @gi eq 'sourceDoc' and normalize-space(.) eq 'Logical divisions not encoded.' and not( /tei:TEI/tei:text//tei:div | /tei:TEI/tei:text//tei:div1 ) )
                                ">
                      A document that uses un-numbered divisions
                      should specify so by using &lt;tagUsage
                      gi="div">Unnumbered divs
                      used.&lt;/tagUsage>; a document that uses
                      numbered divisions should specify so by using
                      &lt;tagUsage gi="div1">Numbered divs
                      used.&lt;/tagUsage>; a document that uses
                      neither should specify so by using &lt;tagUsage
                      gi="sourceDoc">Logical divisions not
                      encoded.&lt;/tagUsage>.
                      </s:assert>
                    </s:rule>
                  </constraint>
                </constraintSpec>
                <valList type="closed">
                  <valItem ident="div">
                    <gloss>divisions are un-numbered</gloss>
                    <desc>The document uses <gi>div</gi>, not
                    <gi>div1</gi>, <gi>div2</gi>, etc. The content of
                    this <gi>tagUsage</gi> should be <quote>Unnumbered
                    divs used.</quote>.</desc>
                  </valItem>
                  <valItem ident="div1">
                    <gloss>divisions are numbered</gloss>
                    <desc>The document uses <gi>div1</gi> (and perhaps
                    <gi>div2</gi>, etc.), not <gi>div</gi> elements. The content of
                    this <gi>tagUsage</gi> should be <quote>Numbered
                    divs used.</quote>.</desc>
                  </valItem>
                  <valItem ident="sourceDoc">
                    <gloss>logical divisions not encoded</gloss>
                    <desc>The document uses neither <gi>div</gi> nor
                    <gi>div1</gi> elements, as the logical structure
                    of the document is not encoded. The content of
                    this <gi>tagUsage</gi> should be <quote>Logical
                    divisions not encoded.</quote>.</desc>
                  </valItem>
                </valList>
              </attDef>
            </attList>
          </elementSpec>
          <elementSpec module="header" ident="taxonomy" mode="change">
            <attList>
              <attDef ident="xml:id" mode="change" usage="req"/>
            </attList>
          </elementSpec>
          <elementSpec module="header" ident="teiHeader" mode="change">
            <content>
              <sequence>
                <elementRef key="fileDesc"     minOccurs="1" maxOccurs="1"/>
                <elementRef key="encodingDesc" minOccurs="1" maxOccurs="1"/>
                <elementRef key="profileDesc"  minOccurs="0" maxOccurs="1"/>
                <elementRef key="revisionDesc" minOccurs="0" maxOccurs="1"/>
              </sequence>
            </content>
            <constraintSpec scheme="schematron" ident="no-ab-inside-header">
              <desc>Although TEI allows both <gi>ab</gi> and <gi>p</gi>
              in various places in the TEI header, these Best
              Practices recommend only <gi>p</gi>.</desc>
              <constraint>
                <s:report test=".//tei:ab">Use <gi>p</gi> instead of <gi>ab</gi> in the TEI header. (There are <s:value-of select="count(.//tei:ab)"/> <gi>ab</gi> elements in this header.)</s:report>
              </constraint>
            </constraintSpec>
            <attList>
              <attDef mode="change" ident="xml:lang" usage="req"/>
              <attDef mode="delete" ident="type"/>
            </attList>
          </elementSpec>
          <elementSpec module="header" ident="notesStmt" mode="change">
            <content>
              <!-- Same as P5 except we removed <relatedItem> -->
              <classRef minOccurs="1" maxOccurs="unbounded" key="model.noteLike"/>
            </content>
          </elementSpec>
          <elementSpec module="core" ident="title" mode="change">
            <attList>
              <attDef ident="level" usage="opt" mode="replace">
                <desc>indicates the bibliographic level for a title,
                that is, whether it identifies an article, book,
                journal, series, or unpublished material.</desc>
                <constraintSpec scheme="schematron" ident="titleLevel">
                  <constraint>
                    <s:rule context="tei:fileDesc/tei:titleStmt/tei:title">
                      <s:report test="@level">The @level attribute
                      should not be specified on a &lt;title&gt; within the
                      &lt;titleStmt&gt;.</s:report>
                    </s:rule>
                    <s:rule context="tei:title[parent::tei:seriesStmt]">
                      <s:assert test="@level='s'">When a child of
                      &lt;seriesStmt&gt;, the level of a title should be
                      specified as 's'.</s:assert>
                    </s:rule>
                    <s:rule context="tei:title[ancestor::tei:series]">
                      <s:assert test="@level='s'">When inside the
                      series-level portion of a structured bibliographic
                      citation, the level of a title should be specified
                      as 's'.</s:assert>
                    </s:rule>
                    <s:rule context="tei:title[parent::tei:analytic]">
                      <s:assert test="@level='a'">When a child of
                      &lt;analytic&gt;, the level of a title should be
                      specified as 'a'.</s:assert>
                    </s:rule>
                    <s:rule context="tei:title[parent::tei:monogr]">
                      <s:report test="@level='a'">When a child of
                      &lt;monogr&gt;, the level of a title should not be
                      specified as 'a'.</s:report>
                    </s:rule>
                  </constraint>
                </constraintSpec>
                <valList type="closed">
                  <valItem ident="a">
                    <gloss>analytic</gloss>
                    <desc>analytic title (article, poem, or other item
                    published as part of a larger item)</desc>
                  </valItem>
                  <valItem ident="m">
                    <gloss>monographic</gloss>
                    <desc>monographic title (book, collection, or
                    other item published as a distinct item,
                    including single volumes of multi-volume
                    works)</desc>
                  </valItem>
                  <valItem ident="j">
                    <gloss>journal</gloss>
                    <desc>journal title</desc>
                  </valItem>
                  <valItem ident="s">
                    <gloss>series</gloss>
                    <desc>series title</desc>
                  </valItem>
                  <valItem ident="u">
                    <gloss>unpublished</gloss>
                    <desc>title of unpublished material (including
                    theses and dissertations unless
                    published by a commercial press)</desc>
                  </valItem>
                </valList>
                <remarks>
                  <p>The level of a title is sometimes implied by its
                  context: for example, a title appearing directly
                  within an <gi>analytic</gi> element is <foreign>ipso
                  facto</foreign> of level <val>a</val>, and one
                  appearing within a <gi>series</gi> element of level
                  <val>s</val>. For this reason, the <att>level</att>
                  attribute is not required in contexts where its
                  value can be unambiguously inferred. Where it is
                  supplied in such contexts, its value should not
                  contradict the value implied by its parent element.</p>
                </remarks>
              </attDef>
              <attDef ident="type" usage="rec" mode="replace">
                <datatype minOccurs="1" maxOccurs="1">
                  <dataRef key="teidata.enumerated"/>
                </datatype>
                <constraintSpec scheme="schematron" ident="titleType">
                  <constraint>
                    <s:rule context="tei:titleStmt/tei:title">
                      <s:assert test="@type">Inside the title statment the
                      @type of a &lt;title&gt; should be specified</s:assert>
                    </s:rule>
                    <s:rule context="tei:monogr/tei:title">
                      <s:assert test="@type">Inside a structured
                      bibliographic citation of a mongraphic-level item,
                      @type of a &lt;title&gt; should be specified</s:assert>
                    </s:rule>
                    <s:rule context="tei:analytic/tei:title">
                      <s:assert test="@type">Inside the analytic portion
                      of a structured bibliographic citation, @type of a
                      &lt;title&gt; should be specified</s:assert>
                    </s:rule>
                  </constraint>
                </constraintSpec>
                <valList type="semi">
                  <valItem ident="main">
                    <desc>main title</desc>
                  </valItem>
                  <valItem ident="sub">
                    <gloss>subordinate</gloss>
                    <desc>subtitle, title of part</desc>
                  </valItem>
                  <valItem ident="alt">
                    <gloss>alternate</gloss>
                    <desc>alternate title, often in another language,
                    by which the work is also known</desc>
                  </valItem>
                  <valItem ident="short">
                    <desc>abbreviated form of title</desc>
                  </valItem>
                  <valItem ident="desc">
                    <gloss>descriptive</gloss>
                  </valItem>
                  <valItem ident="translated">
                    <desc>a translation of a title</desc>
                  </valItem>
                  <valItem ident="marc245a">
                    <desc>used for the title proper and alternative
                    title according to the national cataloging
                    code</desc>
                  </valItem>
                  <valItem ident="marc245b">
                    <desc>used for the the remainder of the title
                    information — parallel titles, titles subsequent
                    to the first, and other title information —
                    according to the national cataloging code)</desc>
                  </valItem>
                  <valItem ident="marc245c">
                    <desc>used for the statement of responsibility
                    according to the national cataloging code</desc>
                  </valItem>
                  <valItem ident="uniform">
                    <desc>used for a uniform title according to the
                    national cataloging code</desc>
                  </valItem>
                </valList>
              </attDef>
            </attList>
          </elementSpec>
          <classSpec type="atts" ident="att.BPTL.div.typed" mode="add">
            <desc versionDate="2017-04-02" xml:lang="en">provides a recommended set of values describing common text divisions which can be used to classify sections of text.</desc>
            <attList>
              <attDef ident="type" usage="rec">
                <datatype>
                  <dataRef key="teidata.enumerated"/>
                </datatype>
                <defaultVal>section</defaultVal>
                <!-- When changing values for the @type attribute on
                     divs, remember to make the parallel adjustments
                     to the list that appears in the prose of the BPTL
                     (in main-driver.odd), so that the values remain
                     in sync -->
                <valList type="semi">
                  <valItem ident="preface">
                    <desc>A section of the front matter that does not carry a more specific designation. Usually appears in front matter.</desc>
                  </valItem>
                  <valItem ident="foreword">
                    <desc>Usually appears in front matter.</desc>
                  </valItem>
                  <valItem ident="acknowledgement">
                    <desc>Usually appears in front matter.</desc>
                  </valItem>
                  <valItem ident="dedication">
                    <desc>A dedication. Usually appears in front matter.</desc>
                  </valItem>
                  <valItem ident="abstract">
                    <desc>Usually appears in front matter.</desc>
                  </valItem>
                  <valItem ident="contents">
                    <desc>A table of contents. Usually appears in front matter.</desc>
                  </valItem>
                  <valItem ident="frontispiece">
                    <desc>A portrait or other image (usually of the author, usually full page) printed at the front of a document. Usually appears in front matter.</desc>
                  </valItem>
                  <valItem ident="appendix">
                    <desc>Usually appears in back matter.</desc>
                  </valItem>
                  <valItem ident="glossary">
                    <desc>Usually appears in back matter.</desc>
                  </valItem>
                  <valItem ident="notes">
                    <desc>Usually appears in back matter.</desc>
                  </valItem>
                  <valItem ident="bibliography">
                    <desc>Usually appears in back matter.</desc>
                  </valItem>
                  <valItem ident="index">
                    <desc><desc>An alphabetical listing of the topics in a document, usually with accompanying page references</desc> Usually appears in back matter.</desc>
                  </valItem>
                  <valItem ident="colophon">
                    <desc>A short inscription, typically at the end of a book or manuscript, containing the title, printer, date and place of printing, etc. Usually appears in back matter.</desc>
                  </valItem>
                  <valItem ident="book">
                    <desc>A major structural component of a long work, identified explicitly in
                    the text as a "book".se</desc>
                  </valItem>
                  <valItem ident="chapter">
                    <desc>A chapter, typically in a prose document.</desc>
                  </valItem>
                  <valItem ident="part">
                    <desc>A major component of a work, containing further subdivisions.</desc>
                  </valItem>
                  <valItem ident="section">
                    <desc>A generic section of a larger work.</desc>
                  </valItem>
                  <valItem ident="subsection">
                    <desc>A generic subdivision of a section.</desc>
                  </valItem>
                  <valItem ident="volume">
                    <desc>A single printed volume in a multi-volume work.</desc>
                  </valItem>
                  <valItem ident="endnotes">
                    <desc>A section containing endnotes for the document.</desc>
                  </valItem>
                  <valItem ident="advert">
                    <desc>An advertisement for a printed work or other product.</desc>
                  </valItem>
                  <valItem ident="corrigenda">
                    <desc>A section describing corrections to be made to the document.</desc>
                  </valItem>
                  <valItem ident="docAuthorization">
                    <desc>A statement indicating that the document's printing was officially
                    authorized.</desc>
                  </valItem>
                  <valItem ident="imprimatur">
                    <desc>A formal indication (usually on the title page or in the front matter)
                    that the document has received official license to be printed.</desc>
                  </valItem>
                  <valItem ident="drama">
                    <desc>A dramatic text.</desc>
                  </valItem>
                  <valItem ident="dramaPart">
                    <desc>A portion of a drama other than a prologue, act, scene, or epilogue.</desc>
                  </valItem>
                  <valItem ident="act">
                    <desc>An act in a dramatic text.</desc>
                  </valItem>
                  <valItem ident="scene">
                    <desc>A scene in a dramatic text.</desc>
                  </valItem>
                  <valItem ident="castlist">
                    <desc>A list of characters in a dramatic text.</desc>
                  </valItem>
                  <valItem ident="poem">
                    <desc>A poem.</desc>
                  </valItem>
                  <valItem ident="poemGroup">
                    <desc>A group of two or more poems under a common heading.</desc>
                  </valItem>
                  <valItem ident="argument">
                    <desc>A short passage at the start of a document or section giving a prose
                    description of its contents.</desc>
                  </valItem>
                  <valItem ident="entry">
                    <desc>An entry in a document that is organized as a log or diary with dated
                    entries.</desc>
                  </valItem>
                  <valItem ident="concluding">
                    <desc>Any section of the back matter that does not carry a more specific
                    designation; includes afterwords, epilogues, codas, etc.</desc>
                  </valItem>
                  <valItem ident="epigraph">
                    <desc>A short quotation at the start of a document or section, often
                    accompanied by an attribution.</desc>
                  </valItem>
                  <valItem ident="prologue">
                    <desc>An opening section of a literary work (typically drama or poetry).</desc>
                  </valItem>
                  <valItem ident="epilogue">
                    <desc>A short concluding section, usually of a dramatic or fictional work.</desc>
                  </valItem>
                  <valItem ident="narrative">
                    <desc>An embedded narrative.</desc>
                  </valItem>
                  <valItem ident="nonfictionProse">
                    <desc>A text intended to be nonfiction consisting primarily of prose.</desc>
                  </valItem>
                  <valItem ident="novelPart">
                    <desc>A portion of a novel.</desc>
                  </valItem>
                  <valItem ident="calendar">
                    <desc>A formal calendar; a document or document section identifying itself as a calendar.</desc>
                  </valItem>
                  <valItem ident="essay">
                    <desc>A short prose non-fiction document.</desc>
                  </valItem>
                  <valItem ident="timeline">
                    <desc>A timeline.</desc>
                  </valItem>
                  <valItem ident="examination">
                    <desc>A section that is identified explicitly as an <soCalled>examination</soCalled> in a text that uses this as a main structural division.</desc>
                  </valItem>
                  <valItem ident="letter">
                    <desc>Any document in epistolary form, i.e. addressed by a sender to a recipient.</desc>
                  </valItem>
                  <valItem ident="prayer">
                    <desc>A prayer.</desc>
                  </valItem>
                  <valItem ident="recipe">
                    <desc>A recipe in a cookbook.</desc>
                  </valItem>
                  <valItem ident="speech">
                    <desc>A section described as a speech or public lecture, in a document that uses this as a main structural division.</desc>
                  </valItem>
                  <valItem ident="tract">
                    <desc>A short treatise in pamphlet form often on a religious subject.</desc>
                  </valItem>
                </valList>
              </attDef>
            </attList>
          </classSpec>
          <!-- class deletions, in alphabetical order: -->
        </specGrp>
      </div>
    </div>
