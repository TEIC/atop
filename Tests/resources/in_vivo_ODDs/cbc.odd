<?xml version="1.0" encoding="UTF-8"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
  <teiHeader>
    <fileDesc>
      <titleStmt>
        <title xml:lang="fr"
          >Schéma et documentation de l’édition structurée des procès-verbaux du Conseil des bâtiments civils</title>
