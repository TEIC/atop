           <!-- SCHEMA, ELEMENTS -->
            <div>
                <head>Appendix</head>
            <schemaSpec ident="dracor" docLang="en" prefix="tei_" xml:lang="en">
                <moduleRef key="core"
                    except="abbr add addrLine address analytic biblScope biblStruct binaryObject cb choice citedRange corr del desc distinct divGen editor email expan gap gb gloss headItem headLabel hi imprint index item label list listBibl measure measureGrp media meeting mentioned milestone monogr num orig postBox postCode ptr pubPlace q reg relatedItem rs said series sic soCalled street teiCorpus textLang time unclear"/>
                <moduleRef key="tei" except=""/>
                <moduleRef key="header"
                    except="abstract appInfo application authority biblFull cRefPattern calendar calendarDesc catDesc catRef category classDecl correction correspAction correspContext correspDesc creation distributor edition editionStmt editorialDecl encodingDesc extent funder geoDecl handNote hyphenation interpretation langUsage language listPrefixDef namespace normalization note notesStmt prefixDef principal projectDesc punctuation quotation refState refsDecl rendition samplingDecl schemaRef scriptNote segmentation seriesStmt sponsor stdVals styleDefDecl tagUsage tagsDecl taxonomy xenoData"/>
                <moduleRef key="textstructure"
                    except="argument byline closer div1 div2 div3 div4 div5 div6 div7 docDate docEdition floatingText group imprimatur opener postscript salute titlePage"/>
                <moduleRef key="drama"
                    except="camera caption epilogue move prologue sound tech view"/>
                <moduleRef key="namesdates"
                    except="affiliation age birth bloc climate country death district education event faith floruit geo geogFeat geogName langKnowledge langKnown listEvent listNym listOrg listPlace location nationality nym occupation offset org orgName persona place placeName population region residence roleName settlement sex socecStatus state terrain trait"/>
                <moduleRef key="corpus"
                    except="activity channel constitution derivation domain factuality interaction locale preparedness purpose setting settingDesc textDesc"
                />
                <moduleRef key="linking" include="ab"/>
                <moduleRef key="figures" include="figure"/>
                <moduleRef key="analysis"/>
            </schemaSpec>
            
            
